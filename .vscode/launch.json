{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node Debug",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "cwd": "${fileDirname}"
    },
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5500",  // Live Server„ÅÆURL
      "webRoot": "${workspaceFolder}"
    }
  ]
  
}
